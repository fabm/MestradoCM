<?xml version="1.0" encoding="UTF-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="res-add-cliente" type="res-add-cliente"/>

    <xs:complexType name="res-add-cliente">
        <xs:sequence>
            <xs:element type="xs:long" name="id" maxOccurs="1" minOccurs="1"/>
            <xs:element type="type-response" name="type-response" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:simpleType name="type-response">
        <xs:restriction base="xs:string">
            <xs:enumeration value="OK"/>
            <xs:enumeration value="ERRO"/>
        </xs:restriction>
    </xs:simpleType>


</xs:schema>