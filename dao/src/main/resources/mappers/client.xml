<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper>
    <select id="selectCliente" resultType="Cliente">
        SELECT * from CLIENTE where CLIENTE.ID_CLIENTE = #{id}
    </select>
</mapper>